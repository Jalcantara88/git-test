k={this.toggleModal}>\r\n                <i className=\"fa fa-pencil fa-lg\" />Submit Comment\r\n                </Button>\r\n\r\n                <Modal isOpen={this.state.isModalOpen} toggle={this.toggleModal}>\r\n                    <ModalHeader toggle={this.toggleModal}>Login</ModalHeader>\r\n                    <ModalBody>\r\n{/*CONTACT FORM*/}\r\n                    <LocalForm onSubmit={values => this.handleSubmit(values)}>\r\n                            <div className=\"form-group\">\r\n                                <Label htmlFor=\"rating\" md={2}>Rating</Label>\r\n                                \r\n                                    <Control.select name=\"rating\" model=\".rating\"\r\n                                    className=\"form-control\">\r\n                                        <option>1</option>\r\n                                        <option>2</option>\r\n                                        <option>3</option>\r\n                                        <option>4</option>\r\n                                        <option>5</option>\r\n                                    </Control.select>\r\n\r\n                                    \r\n                            </div>\r\n                            <div className=\"form-group\">\r\n                                <Label htmlFor=\"author\" md={2}>Author</Label>\r\n                                \r\n                                    <Control.text model=\".author\" id=\"author\" name=\"author\"\r\n                                        placeholder=\"Author\" className=\"form-control\" \r\n                                        validators={{\r\n                                            required,\r\n                                            minLength: minLength(2),\r\n                                            maxLength: maxLength(15),\r\n                                          }}\r\n                                    /> \r\n\r\n                                    <Errors\r\n                                        className=\"text-danger\"\r\n                                        model=\".author\"\r\n                                        show=\"touched\"\r\n                                        component=\"div\"\r\n                                        messages={{\r\n                                        required: \"Required\",\r\n                                        minLength: \"Must be at least 2 characters\",\r\n                                        maxLength: \"Must be 15 characters or less\",\r\n                                        }}\r\n                                    />                      \r\n                            </div>\r\n                            <div className=\"form-group\">\r\n                                <Label htmlFor=\"text\" md={2}>Text</Label>\r\n        \r\n                                    <Control.textarea model=\".text\" id=\"text\" name=\"text\"\r\n                                        rows=\"6\" className=\"form-control\"\r\n                                    />\r\n                            </div>\r\n                                \r\n                            <Button type=\"submit\" color=\"primary\">\r\n                                Submit\r\n                            </Button>\r\n                                   \r\n                        </LocalForm>\r\n                    </ModalBody>\r\n                </Modal>\r\n            </React.Fragment>\r\n            \r\n\r\n\r\n        );\r\n    }\r\n}\r\n\r\nfunction RenderComments({comments, addComment, campsiteId}) {\r\n        if(comments) {\r\n            return(\r\n                <div className=\"col-md-5 m-1\">\r\n                    <h4>Comments</h4>\r\n                    {comments.map(\r\n                        comments => {\r\n                            return(\r\n                                <div key={comments.id} className=\"col-md m-3\">\r\n                                    \r\n                                        <div>\r\n                                            {comments.author} , {new Intl.DateTimeFormat('en-US', { year: 'numeric', month: 'short', day: '2-digit'}).format(new Date(Date.parse(comments.date)))}\r\n                                        </div>\r\n                                        <strong>\r\n                                            {comments.text}\r\n                                        </strong>\r\n                                    \r\n                                </div>\r\n                            );\r\n                        }\r\n                    )}\r\n                    <CommentForm campsiteId={campsiteId} addComment={addComment} />\r\n\r\n\r\n                </div>\r\n            );\r\n        }\r\n    return <div/>;\r\n}\r\n\r\nfunction RenderCampsite({campsite}) {\r\n    return (\r\n        <div className=\"col-md-5 m-1\">\r\n            <Card>\r\n                <CardImg top src={campsite.image} alt={campsite.name}/>\r\n                <CardBody>\r\n                    <CardTitle>{campsite.name}</CardTitle>\r\n                    <CardText>{campsite.description}</CardText>\r\n                </CardBody>\r\n            </Card>\r\n        </div>\r\n    );\r\n}\r\n\r\nfunction CampsiteInfo(props) {\r\n        if(props.isLoading) {\r\n            return(\r\n                <div className=\"container\">\r\n                    <div className=\"row\">\r\n                        <Loading />\r\n                    </div>\r\n                </div>\r\n            );\r\n        }\r\n        if(props.errMess) {\r\n            return(\r\n                <div className=\"container\">\r\n                    <div className=\"row\">\r\n                        <div className=\"col\">\r\n                            <h4>{props.errMess}</h4>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            );\r\n        }\r\n        if(props.campsite) {\r\n            return (\r\n                <div>\r\n                    <div className=\"container\">\r\n                    <div className=\"row\">\r\n                        <div className=\"col\">\r\n                            <Breadcrumb>\r\n                                <BreadcrumbItem>\r\n                                <Link to=\"/directory\">Directory</Link>\r\n                                </BreadcrumbItem>\r\n                                <BreadcrumbItem active>{props.campsite.name}</BreadcrumbItem>\r\n                            </Breadcrumb>\r\n                            <h2>{props.campsite.name}</h2>\r\n                            <hr />\r\n                        </div>\r\n                    </div>\r\n\r\n                        <div className=\"row\">\r\n                            <RenderCampsite campsite={props.campsite}/>\r\n\r\n                            <RenderComments comments={props.comments} \r\n                                addComment={props.addComment}\r\n                                campsiteId={props.campsite.id}\r\n                            />\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            )\r\n        }\r\n        return <div />;\r\n    }\r\n\r\n\r\n\r\n\r\nexport default CampsiteInfo;\r\n","C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\nucampsite\\src\\components\\MainComponent.js",[],"C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\nucampsite\\src\\components\\HeaderComponent.js",[],"C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\nucampsite\\src\\components\\FooterComponent.js",[],"C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\nucampsite\\src\\components\\HomeComponent.js",[],"C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\nucampsite\\src\\components\\ContactComponent.js",["114","115","116","117","118","119","120","121","122"],"C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\nucampsite\\src\\shared\\comments.js",[],"C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\nucampsi